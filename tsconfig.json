{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"pretty": true,
		"sourceMap": true,
		"allowSyntheticDefaultImports": true,
		"outDir": "./dist",
		"baseUrl": "./src"
	},
	"include": ["./src/"],
	"exclude": [
		"node_modules",
		"**/__snapshots__/*",
		"**/*.snap",
		"**/*.spec.ts"
	],
	"files": ["./src/routes/index.ts"]
}
